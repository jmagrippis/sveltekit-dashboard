{
	"name": "sveltekit-dashboard",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm run generate:i18n && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . && inlang lint",
		"format": "prettier --write .",
		"postinstall": "prisma generate",
		"db:seed": "tsx prisma/mocks/seed.ts",
		"generate:i18n": "paraglide-js compile --project ./project.inlang.json",
		"test": "playwright test",
		"update": "ncu -ui --format group --install always"
	},
	"prisma": {
		"seed": "pnpm run db:seed"
	},
	"devDependencies": {
		"@auth/core": "^0.18.3",
		"@auth/prisma-adapter": "^1.0.8",
		"@auth/sveltekit": "^0.3.14",
		"@faker-js/faker": "^8.4.1",
		"@fontsource-variable/inter": "^5.0.19",
		"@fontsource/lusitana": "^5.0.12",
		"@inlang/cli": "^1.20.0",
		"@inlang/paraglide-js": "1.11.1",
		"@playwright/test": "^1.45.1",
		"@prisma/client": "^5.16.2",
		"@sveltejs/adapter-vercel": "^5.4.1",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@types/negotiator": "^0.6.3",
		"@types/node": "^20.14.10",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"autoprefixer": "^10.4.19",
		"chart.js": "^4.4.3",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.42.0",
		"lucide-svelte": "^0.407.0",
		"negotiator": "^0.6.3",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8.4.39",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"prisma": "^5.16.2",
		"sharp": "^0.33.4",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"tsx": "^4.16.2",
		"typescript": "^5.5.3",
		"vite": "^5.3.3",
		"vite-imagetools": "^7.0.4",
		"zod": "^3.23.8"
	},
	"type": "module",
	"engines": {
		"node": "18.x"
	},
	"volta": {
		"node": "18.18.2"
	}
}
